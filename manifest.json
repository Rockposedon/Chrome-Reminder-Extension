{
  "manifest_version": 3,
  "name": "Reminder Extension",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "notifications"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon64.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon64.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "audio1.mp3"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ]
}

